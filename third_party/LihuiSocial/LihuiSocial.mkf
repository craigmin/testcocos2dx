# Subproject file for extension LihuiSocial.
#
# Include this subproject in your App to enable access to the LihuiSocial API
#
# NOTE: This file was originally written by the extension builder, but will not
# be overwritten (unless --force is specified) and is intended to be modified.

#
# Running the extension builder with --platform can cause additions to this file
# even after modification, to support additional platforms

display_name "Extensions/LihuiSocial"

includepath h

files
{
    (h)
    LihuiSocial.h
	#MobClickCpp.h
	#jni.h
    ["interface"]
    (interface)
    LihuiSocial_interface.cpp
	#MobClickCpp.cpp
    LihuiSocial.defines.txt
}

defines
{
    IW_TRACE_CHANNEL_LIHUISOCIAL_VERBOSE=2
}

if {{ not defined IW_MKF_IWCRT}}
    subproject iwutil

#Autogenerated section for android platform support. Deleting these two comment
#lines will cause the extension builder to re-output this section.
#Include the extension's jar file during deployment
deployments
{
    android-external-jars="lib/android/LihuiSocial.jar"
    android-so="lib/android/libLihuiSocial.so"
	android-external-jars="libammsdk.jar"
	android-external-jars="umeng_sdk.jar"
	android-extra-application-manifest="LihuiSocial.xml"
	#android-extra-application-manifest="Umeng.xml"
}
